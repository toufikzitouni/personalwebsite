{
  "name": "personalwebsite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://toufikzitouni.github.io/personalwebsite",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run watch:css\"",
    "build": "vite build",
    "build:css": "npx tailwindcss build src/main.css -o src/output.css",
    "watch:css": "npx tailwindcss -i src/main.css -o src/output.css --watch",
    "preview": "vite preview",
    "deploy": "yarn build && cd dist && git add . && git commit -m 'Deploying dist content' && git push -f origin main && cd .."
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "concurrently": "^8.2.2",
    "gh-pages": "^6.1.1",
    "svelte": "^4.2.18",
    "vite": "^5.3.1"
  },
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "svelte-routing": "^2.13.0",
    "tailwindcss": "^3.4.4"
  }
}
